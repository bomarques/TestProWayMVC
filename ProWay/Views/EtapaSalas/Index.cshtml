@model IEnumerable<ProWay.Models.EtapaSala>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Etapa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAlunoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdSalaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Etapa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAlunoNavigation.IdAluno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSalaNavigation.IdSala)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdEtapa">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdEtapa">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdEtapa">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
